<?xml version="1.0" encoding="utf-8"?>
<table name="plugin_error_log" xmlns="http://ubp.framework.longestvision.com/configuration/application/database/mysql/table">
	<fields>
		<field name="id">
			<type name="varchar">
				<length>32</length>
				<allowNull>false</allowNull>
			</type>
		</field>
		<field name="requestId">
			<type name="varchar">
				<length>32</length>
				<allowNull>false</allowNull>
			</type>
		</field>
		<field name="errorId">
			<type name="varchar">
				<length>32</length>
				<allowNull>false</allowNull>
			</type>
		</field>
	</fields>
	<indexes>
		<unique>
			<field>requestId</field>
			<field>errorId</field>
		</unique>
		<primary>
			<field>id</field>
		</primary>
	</indexes>
</table>