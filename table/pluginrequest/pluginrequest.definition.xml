<?xml version="1.0" encoding="utf-8"?>
<table name="plugin_request" xmlns="http://ubp.framework.longestvision.com/configuration/application/database/mysql/table">
	<fields>
		<field name="id">
			<type name="varchar">
				<length>32</length>
				<allowNull>false</allowNull>
			</type>
		</field>
		<field name="url">
			<type name="varchar">
				<length>200</length>
				<allowNull>false</allowNull>
			</type>
		</field>
		<field name="queryString">
			<type name="varchar">
				<length>200</length>
				<allowNull>true</allowNull>
			</type>
		</field>
	</fields>
	<indexes>
		<unique>
			<field>url</field>
			<field>queryString</field>
		</unique>
		<primary>
			<field>id</field>
		</primary>
	</indexes>
</table>